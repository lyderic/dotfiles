#!/usr/bin/env -S just --justfile

alias h := _help

_help:
	@{{justfile()}} --list --unsorted --alias-style left --color always \
		--list-heading='' --list-prefix=' ' \
		| sed -e 's/alias: //' | awk 'NF'

test:
	# something to test

[private]
v:
	{{justfile()}} --evaluate

set export
set shell := ["bash","-uc"]
# vim: ft=just
